OUTPUT PORT "USBFifoData_DO[0]" LOAD 1.0 pF ;
OUTPUT PORT "USBFifoChipSelect_SBO" LOAD 1.0 pF ;
OUTPUT PORT "USBFifoWrite_SBO" LOAD 1.0 pF ;
OUTPUT PORT "USBFifoRead_SBO" LOAD 1.0 pF ;
OUTPUT PORT "USBFifoPktEnd_SBO" LOAD 1.0 pF ;
OUTPUT PORT "USBFifoAddress_DO[0]" LOAD 1.0 pF ;
OUTPUT PORT "USBFifoAddress_DO[1]" LOAD 1.0 pF ;
OUTPUT PORT "LED1_SO" LOAD 1.0 pF ;
OUTPUT PORT "LED2_SO" LOAD 1.0 pF ;
OUTPUT PORT "LED3_SO" LOAD 1.0 pF ;
OUTPUT PORT "LED4_SO" LOAD 1.0 pF ;
