// APS data (chip)
LOCATE COMP "APSChipRowSRClock_SO" SITE "D9" ;
LOCATE COMP "APSChipRowSRIn_SO" SITE "C9" ;
LOCATE COMP "APSChipColSRClock_SO" SITE "D11" ;
