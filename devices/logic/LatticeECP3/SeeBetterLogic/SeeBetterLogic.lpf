// APS data (external ADC)
LOCATE COMP "APSADCData_DI[9]" SITE "J15" ;
LOCATE COMP "APSADCData_DI[8]" SITE "H16" ;
LOCATE COMP "APSADCData_DI[7]" SITE "G15" ;
