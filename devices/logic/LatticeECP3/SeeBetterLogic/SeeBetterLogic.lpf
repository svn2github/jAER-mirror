LOCATE COMP "APSADCOutputEnable_SBO" SITE "C10" ;
LOCATE COMP "APSADCStandby_SO" SITE "D10" ;

DEFINE PORT GROUP "APSADCData_DI"
"APSADCData_DI[9]" 
"APSADCData_DI[8]" 
"APSADCData_DI[7]" 
